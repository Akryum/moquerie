{
  "name": "@moquerie/app",
  "type": "module",
  "version": "0.6.2",
  "description": "Nuxt app frontend for moquerie",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
    "url": "https://github.com/Akryum/moquerie.git",
    "type": "git",
    "directory": "packages/app"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./index.d.mts",
      "default": "./index.mjs"
    },
    "./*": "./*"
  },
  "types": "./index.d.mts",
  "files": [
    ".output",
    "index.d.ts",
    "index.mjs"
  ],
  "scripts": {
    "build": "nuxt prepare && nuxt build",
    "dev": "nuxt prepare && nuxt dev --dotenv .env.playground",
    "dev:rest": "nuxt prepare && nuxt dev --dotenv .env.playground.rest",
    "test:types": "nuxt typecheck"
  },
  "peerDependencies": {
    "@moquerie/core": "workspace:^",
    "graphql": ">15"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@iconify/json": "^2.2.150",
    "@moquerie/core": "workspace:^",
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/ui": "^2.18.4",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/envinfo": "^7.8.3",
    "@vueuse/nuxt": "^11.0.1",
    "@vueuse/router": "^11.0.1",
    "envinfo": "^7.11.0",
    "floating-vue": "^5.2.2",
    "graphql": "^16.9.0",
    "jiti": "^1.21.0",
    "just-clone": "^6.2.0",
    "just-diff-apply": "^5.5.0",
    "launch-editor": "^2.8.1",
    "monaco-editor": "^0.50.0",
    "monaco-graphql": "^1.6.0",
    "nanoid": "^5.0.4",
    "nuxt": "^3.12.4",
    "pathe": "^1.1.2",
    "pinia": "^2.1.7",
    "superjson": "^2.2.1",
    "typescript": "^5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
