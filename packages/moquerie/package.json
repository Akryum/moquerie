{
  "name": "moquerie",
  "type": "module",
  "version": "0.0.0",
  "description": "Create API & backend mocks with ease",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
    "url": "https://github.com/Akryum/moquerie.git",
    "type": "git",
    "directory": "packages/moquerie"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "build": "pnpm run \"/^build:/\"",
    "build:lib": "rm -rf ./dist && tsc --project tsconfig.lib.json",
    "build:nuxt": "nuxt prepare && nuxt build",
    "dev": "nuxt prepare && nuxt dev",
    "dev:lib": "tsc --project tsconfig.lib.json --watch"
  },
  "dependencies": {
    "c12": "^1.5.1",
    "fast-glob": "^3.3.2",
    "jiti": "^1.21.0",
    "jscodeshift": "^0.15.1",
    "pathe": "^1.1.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.150",
    "@nuxt/devtools": "latest",
    "@nuxt/ui": "^2.8.1",
    "@vueuse/nuxt": "^10.6.1",
    "graphql": "^16.8.1",
    "launch-editor": "^2.6.1",
    "monaco-editor": "^0.44.0",
    "monaco-graphql": "^1.4.0",
    "nuxt": "^3.8.2",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "graphql": ">15"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  }
}
