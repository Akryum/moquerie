{
  "name": "moquerie",
  "type": "module",
  "version": "0.0.0",
  "description": "Create API & backend mocks with ease",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
    "url": "https://github.com/Akryum/moquerie.git",
    "type": "git",
    "directory": "packages/moquerie"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "scripts": {
    "build": "pnpm run \"/^build:/\"",
    "build:lib": "rm -rf ./dist && tsc --project tsconfig.lib.json",
    "build:nuxt": "nuxt prepare && nuxt build",
    "dev": "pnpm run \"/^dev:/\"",
    "dev:lib": "tsc --project tsconfig.lib.json --watch",
    "dev:nuxt": "nuxt prepare && nuxt dev"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "c12": "^1.5.1",
    "env-paths": "^3.0.0",
    "envinfo": "^7.11.0",
    "fast-glob": "^3.3.2",
    "jiti": "^1.21.0",
    "jscodeshift": "^0.15.1",
    "nanoid": "^5.0.4",
    "pathe": "^1.1.1",
    "scule": "^1.1.1",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.150",
    "@nuxt/devtools": "latest",
    "@nuxt/ui": "^2.8.1",
    "@pinia/nuxt": "^0.5.1",
    "@types/envinfo": "^7.8.3",
    "@vueuse/nuxt": "^10.6.1",
    "@vueuse/router": "^10.7.0",
    "floating-vue": "2.0.0-beta.24",
    "graphql": "^16.8.1",
    "launch-editor": "^2.6.1",
    "monaco-editor": "^0.44.0",
    "monaco-graphql": "^1.4.0",
    "nuxt": "^3.8.2",
    "pinia": "^2.1.7",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "graphql": ">15"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  }
}
