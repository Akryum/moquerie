{
  "name": "@moquerie/core",
  "type": "module",
  "version": "0.6.1",
  "description": "Create API & backend mocks with ease",
  "author": {
    "name": "Guillaume Chau"
  },
  "repository": {
    "url": "https://github.com/Akryum/moquerie.git",
    "type": "git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./util": {
      "types": "./dist/util/index.d.ts",
      "default": "./dist/util/index.js"
    },
    "./*": "./*"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "graphql": ">15",
    "typescript": ">5.0.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@faker-js/faker": "^8.4.0",
    "@graphql-tools/code-file-loader": "^8.0.3",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/json-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.1",
    "@graphql-tools/schema": "^10.0.2",
    "@graphql-tools/url-loader": "^8.0.1",
    "@graphql-tools/utils": "^10.0.11",
    "@types/express": "^4.17.21",
    "@whatwg-node/server": "^0.9.25",
    "ast-types": "^0.14.2",
    "c12": "^1.5.1",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "env-paths": "^3.0.0",
    "express": "^4.18.2",
    "fast-glob": "^3.3.2",
    "get-port-please": "^3.1.1",
    "git-user-info": "^2.0.3",
    "graphql-yoga": "^5.0.2",
    "jiti": "^1.21.0",
    "just-diff": "^6.0.2",
    "lru-cache": "^10.1.0",
    "nanoid": "^5.0.4",
    "path-to-regexp": "^6.2.1",
    "pathe": "^1.1.2",
    "perfect-debounce": "^1.0.0",
    "picocolors": "^1.0.0",
    "recast": "^0.23.4",
    "scule": "^1.1.1",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "graphql": "^16.8.1",
    "typescript": "^5.3.2"
  }
}
